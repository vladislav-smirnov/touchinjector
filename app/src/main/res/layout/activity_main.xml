<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/native_injection_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/button_margin_start"
        android:text="@string/native_injection"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/instr_injection_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/instrument_injection"
        app:layout_constraintLeft_toLeftOf="@+id/native_injection_button"
        app:layout_constraintTop_toBottomOf="@+id/native_injection_button" />

</androidx.constraintlayout.widget.ConstraintLayout>